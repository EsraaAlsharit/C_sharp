@{
    ViewData["Title"] = "Home Page";
}
@model Wedding

<div class="text-center">
    <h2>
        @Model.WedderOne & @Model.WedderTwo 's wedding
    </h2>
    <h6>Date: @Model.Date</h6>
    <h6>Guests: </h6>
    <ul>
        @for (int i = 0; i < @Model.Guests.Count; i++)
        {
            <li>
                @Model.Guests[i]
            </li>
        }
    </ul>
</div>
